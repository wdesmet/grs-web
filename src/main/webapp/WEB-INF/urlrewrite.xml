<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">
<urlrewrite>
    <rule>
        <from>^/(.+)\.(?!jsp$|css$|js$|html$)(\w+)$</from>
        <set name="net.straininfo2.ext">$2</set>
        <to>/$1</to>
    </rule>
</urlrewrite>